{
     "version": 2,
     "builds": [
       {
         "src": "dist/index.js",
         "use": "@vercel/node"
       }
     ],
     "routes": [
       {
         "src": "/(.*)",
         "dest": "dist/index.js"
       }
     ],
     "functions": {
       "dist/index.js": {
         "maxDuration": 300
       }
     },
     "env": {
       "NODE_ENV": "production"
     }
   }