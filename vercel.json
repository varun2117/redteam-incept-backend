{
     "version": 2,
     "builds": [
       {
         "src": "api/index.ts",
         "use": "@vercel/node"
       }
     ],
     "routes": [
       {
         "src": "/(.*)",
         "dest": "api/index.ts"
       }
     ],
     "functions": {
       "api/index.ts": {
         "maxDuration": 60
       }
     },
     "env": {
       "NODE_ENV": "production"
     }
   }